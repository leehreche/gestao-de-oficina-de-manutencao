<%=  form_with  model: @status, local: true do |form| %>
    
    <div class="form-group">    
        <p>
            <%= form.label :"Tipo Status" %>
            <br>
            <%= form.text_field :tipo_status, class: "form-control", placeholder: "Ex.: 1" %>
        </p>
    </div>

    <div class="form-group"> 
        <p>
            <%= form.label :Descrição %>
            <br>
            <%= form.text_field :descricao , class: "form-control", placeholder: "Ex.: Pendente"%>
        </p>
    </div>

    <p class="text-center">
        <%= form.submit "Enviar Dados", class: "btn btn-dark" %>
    </p>    
<% end %>